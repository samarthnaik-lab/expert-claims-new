{
  "version": 2,
  "builds": [
  {
  "src": "package.json",
  "use": "@vercel/static-build",
  "config": {
  "distDir": "dist"
  }
  }
  ],
  "rewrites": [
  {
  "source": "/api/:path*",
  "destination": "http://31.97.231.232:3000/api/:path*"
  }
  ],
  "routes": [
  {
  "handle": "filesystem"
  },
  {
  "src": "/.*",
  "dest": "/index.html"
  }
  ]
 }
 